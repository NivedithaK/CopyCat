{
  "manifest_version": 2,

  "name": "CopyCat",
  "description": "This extension allows you to copy multiple items.",
  "version": "1.0",
  "content_security_policy":"script-src 'self' https://apis.google.com; object-src 'self'",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
   ]
}
